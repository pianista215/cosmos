  "fetch": [
    {
      "uri": "http://example.com//core-site.xml"
    },
    {
      "uri": "http://example.com//hdfs-site.xml"
    }
  ],
  "healthChecks": [
    {
      "gracePeriodSeconds": 40,
      "ignoreHttp1xx": false,
      "intervalSeconds": 120,
      "maxConsecutiveFailures": 0,
      "path": "/health/status/ecosystem",
      "portIndex": 3,
      "protocol": "HTTP",
      "timeoutSeconds": 20
    },
    {
      "gracePeriodSeconds": 60,
      "ignoreHttp1xx": false,
      "intervalSeconds": 120,
      "maxConsecutiveFailures": 1,
      "path": "/health/status/internal",
      "portIndex": 3,
      "protocol": "HTTP",
      "timeoutSeconds": 20
    }
  ],
  "id": "/crossdata-1",
  "instances": 0,
  "ipAddress": {
    "networkName": "xd"
  },
  "labels": {
    "DCOS_PACKAGE_IS_FRAMEWORK": "false",
