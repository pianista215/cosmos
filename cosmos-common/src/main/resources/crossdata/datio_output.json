      "image": "qa.stratio.com/stratio/crossdata-scala211:2.9.0",
      "network": "USER",
      "parameters": [
        {
          "key": "volume",
          "value": "/etc/krb5.conf:/etc/krb5.conf:ro"
        }
      ],
      "portMappings": [
        {
          "containerPort": 8000,
          "name": "13422"
        },
        {
          "containerPort": 8001,
          "name": "13420"
        },
        {
          "containerPort": 8002,
          "name": "5701"
        },
        {
          "containerPort": 8003,
          "name": "13423"
        }
      ],
      "privileged": false
    },
    "type": "DOCKER",
    "volumes": []
